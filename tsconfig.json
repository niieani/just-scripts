{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "noImplicitAny": false,
    "inlineSourceMap": true,
    "sourceRoot": "src",
    "importHelpers": true,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": "dist"
  }
}